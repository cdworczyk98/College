<Window x:Class="GameSokoban.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameSokoban"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Canvas HorizontalAlignment="Left" Height="321" VerticalAlignment="Top" Width="515" Margin="0,0,0,-2">
        <Canvas.Background>
            <ImageBrush ImageSource="pack://siteoforigin:,,,/Resources/Background_Menu.jpg"/>
        </Canvas.Background>
        <Button x:Name="Button_Start" Content="Play" HorizontalAlignment="Left" VerticalAlignment="Top" Width="213" Height="62" Click="ButtonStart" Canvas.Left="133" Canvas.Top="116"/>
        <Button x:Name="Button_Exit" Content="Exit" HorizontalAlignment="Left" VerticalAlignment="Top" Width="213" Height="62" Click="Button_Exit_Click" Canvas.Left="133" Canvas.Top="183"/>
    </Canvas>
</Window>
